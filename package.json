{
  "name": "merge-conflict-game-extension",
  "displayName": "Merge Conflict Game",
  "description": "Gamifies merge conflict resolution in VS Code.",
  "version": "0.0.1",
  "engines": {
    "vscode": ">=1.60.0"
  },
  "categories": ["Other"],
  "activationEvents": [
    "onStartupFinished",
    "onLanguage:plaintext",
    "onLanguage:git-commit",
    "onLanguage:markdown"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "mergeConflictGame.start",
        "title": "Start Merge Conflict Game"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.60.0",
    "typescript": "^4.4.3",
    "tslint": "^6.1.3"
  },
  "dependencies": {
    "mathjs": "15.1.0",
    "@types/vscode": "^1.60.0",
    "typescript": "^4.4.3",
    "tslint": "^6.1.3"
  }
}
